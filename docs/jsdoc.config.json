{
  "plugins": [
    "plugins/markdown"
  ], 
  "recurseDepth": 10,
  "source": {
      "include": ["../src/typescript/core_api.js", 
        "../src/typescript/extractor.js",
        "../src/typescript/plot.js"
      ],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
      "cleverLinks": true,
      "monospaceLinks": false
  }
}